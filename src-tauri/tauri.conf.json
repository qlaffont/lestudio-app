{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "frontendDist": "Injected by vite-plugin-tauri"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "",
    "longDescription": "",
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "",
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "lestudio-app",
  "mainBinaryName": "lestudio-app",
  "version": "1.1.9",
  "identifier": "com.qlaffont.lestudioapp",
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/qlaffont/lestudio-app/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEQ1REQyRUQyQ0U2MTk3ODgKUldTSWwySE8waTdkMVJ4cnV1aldpUTh6Mnk2Z3RlSUVnRkh2ekFxQ2czMWpsa05wd0NqeGo3UHoK"
    }
  },
  "app": {
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'; connect-src ipc: http://ipc.localhost"
    },
    "windows": [
      {
        "title": "LeStudio App",
        "resizable": true,
        "fullscreen": false
      }
    ],
    "trayIcon": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    }
  }
}